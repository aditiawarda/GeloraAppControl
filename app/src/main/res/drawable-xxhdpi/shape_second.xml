<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="#FFB656" />
            <stroke
                android:width="1dp"
                android:color="#FFB656">
            </stroke>
            <corners
                android:radius="20dp"/>
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#EC9B32" />
            <stroke
                android:width="1dp"
                android:color="#EC9B32">
            </stroke>
            <corners
                android:radius="20dp"/>
        </shape>
    </item>
</selector>